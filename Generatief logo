void setup(){
  size(300,300);
  background(255,255,255);
 
colorMode(HSB, 360, 100, 100);
 
 
 
fill(random(240,340),80,100);
noStroke();
rect(20,20,130,130);
 
fill(random(1,100),100,100);
noStroke();
ellipse(200,130,160,160);
 
fill(random(120,220),100,80);
noStroke();
triangle(60,280,200,130,250,280);
 
}
